  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RehamMuhammad/Solana-Verification-API.gi/issues"
  },
  "homepage": "https://github.com/RehamMuhammad/Solana-Verification-API.gi#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "bs58": "^5.0.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "mongodb": "^4.5.0",
    "mongoose": "^6.3.1",

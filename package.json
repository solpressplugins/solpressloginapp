{
  "name": "solanaapi",
  "version": "1.0.0",
  "description": "A RESTful API with Node.js and Express to verify Logging Message.",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon ./src/index.js --exec babel-node -e js",
    "build": "npm run clean && npm run build-babel",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "start": "node build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RehamMuhammad/Solana-Verification-API.gi.git"
  },
